<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @sighting.id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @sighting.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @sighting.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">SightedAt</td>
        <td class="itemValue"><%= @sighting.sighted_at%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ReportedAt</td>
        <td class="itemValue"><%= @sighting.reported_at%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Location</td>
        <td class="itemValue"><%= @sighting.location%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Shape</td>
        <td class="itemValue"><%= @sighting.shape%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Duration</td>
        <td class="itemValue"><%= @sighting.duration%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Description</td>
        <td class="itemValue"><%= @sighting.description%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Coord</td>
        <td class="itemValue"><%= @sighting.coord%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Links</td>
        <td class="itemValue"><%= @sighting.links%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Source</td>
        <td class="itemValue"><%= @sighting.source%></td>
      </tr>
      
    </table>
  </div>

</div>
